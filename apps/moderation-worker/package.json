{
  "name": "xivdyetools-moderation-worker",
  "version": "1.1.4",
  "private": true,
  "description": "XIV Dye Tools Discord Moderation Bot - Cloudflare Workers Edition",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:production": "wrangler deploy --env production",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "register-commands": "npx tsx scripts/register-commands.ts"
  },
  "keywords": ["discord", "bot", "ffxiv", "cloudflare-workers", "moderation"],
  "author": "Flash Galatine",
  "license": "MIT",
  "dependencies": {
    "@cloudflare/workers-types": "^4.20260207.0",
    "@xivdyetools/auth": "workspace:*",
    "@xivdyetools/logger": "workspace:*",
    "@xivdyetools/rate-limiter": "workspace:*",
    "@xivdyetools/types": "workspace:*",
    "hono": "^4.11.10"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "@vitest/coverage-v8": "^4.0.18",
    "@xivdyetools/test-utils": "workspace:*",
    "discord-interactions": "^4.4.0",
    "dotenv": "^17.2.4",
    "tsx": "^4.21.0",
    "vitest": "^4.0.18",
    "wrangler": "^4.66.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
